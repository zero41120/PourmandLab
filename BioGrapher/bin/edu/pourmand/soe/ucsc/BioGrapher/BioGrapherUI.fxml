<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox maxHeight="1080.0" maxWidth="1920.0" minHeight="600.0" minWidth="800.0" prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.pourmand.soe.ucsc.BioGrapher.GUIController">
	<children>
		<MenuBar>
			<menus>
				<Menu mnemonicParsing="false" text="File">
					<items>
						<MenuItem mnemonicParsing="false" onAction="#actionBrowse" text="Browse" />
                  <MenuItem mnemonicParsing="false" onAction="#actionClearData" text="Clear Data" />
                  <MenuItem mnemonicParsing="false" onAction="#actionExportCalibration" text="Export Data" />
					</items>
				</Menu>
				<Menu mnemonicParsing="false" text="Edit">
					<items>
                  <MenuItem mnemonicParsing="false" onAction="#actionComparisonPlot" text="Comparsion Plot" />
                  <MenuItem mnemonicParsing="false" onAction="#actionCalibrationPlot" text="Calibration Plot" />
						<MenuItem mnemonicParsing="false" onAction="#actionEdit" text="Edit Concentration" />
					</items>
				</Menu>
				<Menu mnemonicParsing="false" text="Help">
					<items>
						<MenuItem mnemonicParsing="false" text="About" />
					</items>
				</Menu>
			</menus>
		</MenuBar>
      <ToolBar prefHeight="40.0" prefWidth="200.0">
         <items>
												<Button fx:id="btnBrowse" mnemonicParsing="false" onAction="#actionBrowse" prefHeight="20.0" prefWidth="100.0" text="Browse" />
            <Button fx:id="btnCalibrationPlot" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#actionCalibrationPlot" text="Calibration Plot" />
            <Button fx:id="btnComparisonPlot" layoutX="378.0" layoutY="10.0" mnemonicParsing="false" onAction="#actionComparisonPlot" text="Comparison Plot" />
            <AnchorPane prefHeight="27.0" prefWidth="226.0">
               <children>
                  <ProgressBar fx:id="pbMainProgressBar" layoutX="57.0" progress="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
               </children></AnchorPane>
         </items>
      </ToolBar>
      <LineChart fx:id="chartMainChart" legendSide="RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="40.0" minWidth="80.0" prefHeight="650.0">
        <xAxis>
          <NumberAxis fx:id="charMainxAxis" side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="charMainyAxis" side="LEFT" />
        </yAxis>
      </LineChart>
      <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="299.0" prefWidth="1024.0">
         <children>
            <TitledPane animated="false" text="Concetration">
               <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="161.0" prefWidth="200.0">
                     <children>
      										<Button fx:id="btnEstimateConcentration" alignment="CENTER" contentDisplay="TOP" layoutX="17.0" layoutY="76.0" mnemonicParsing="false" onAction="#actionEstimateConcentration" text="Estimate Concentration" AnchorPane.leftAnchor="0.0" />
      												<Label layoutX="2.0" layoutY="6.0" text="Input Charge (nA):" AnchorPane.leftAnchor="5.0" AnchorPane.topAnchor="5.0" />
      												<TextField fx:id="txfCInputCharge" layoutX="16.0" layoutY="40.0" AnchorPane.leftAnchor="0.0" />
                        <Label fx:id="labEstimateConcentration" layoutX="26.0" layoutY="112.0" prefHeight="27.0" prefWidth="167.0" text="" AnchorPane.leftAnchor="0.0" />
                     </children>
                  </AnchorPane>
               </content>
            </TitledPane>
            <TitledPane animated="false" prefHeight="175.0" prefWidth="821.0" text="Data Provider Status">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="162.0" prefWidth="600.0">
                     <children>
                        <TextFlow fx:id="txfwReport" focusTraversable="true" layoutX="21.4" layoutY="20.4" pickOnBounds="false" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                        <Button fx:id="btnReloadStatus" layoutX="100.0" layoutY="115.0" mnemonicParsing="false" onAction="#actionReloadStatus" text="Reload Status" AnchorPane.bottomAnchor="0.0" />
                        <Button fx:id="btnEdit" layoutX="250.0" layoutY="105.0" mnemonicParsing="false" onAction="#actionEdit" prefHeight="20.0" prefWidth="100.0" text="Edit" AnchorPane.bottomAnchor="0.0" />
            												<Button fx:id="btnClearData" layoutX="403.0" layoutY="105.0" mnemonicParsing="false" onAction="#actionClearData" prefHeight="20.0" prefWidth="100.0" text="Clear Data" AnchorPane.bottomAnchor="0.0" />
                     </children>
                  </AnchorPane>
              </content>
            </TitledPane>
         </children>
      </HBox>
	</children>
</VBox>
